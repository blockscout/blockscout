<% tags = if assigns[:tags], do: @tags, else: [] %>
<%= if is_omni_bridge?(@address_hash) do %>
  <%= render BlockScoutWeb.FormView, "_tag.html", text: "OMNI", additional_classes: ["omni", "ml-1"] %>
  <%= render BlockScoutWeb.FormView, "_tag.html", text: "bridge", additional_classes: ["bridge", "ml-1"] %>
<% end %>
<%= if is_omni_eth_bridge?(@address_hash) do %>
  <%= render BlockScoutWeb.FormView, "_tag.html", text: "ETH", additional_classes: ["destination-eth", "ml-1"] %>
<% end %>
<%= if is_omni_bsc_bridge?(@address_hash) do %>
  <%= render BlockScoutWeb.FormView, "_tag.html", text: "BSC", additional_classes: ["destination-bsc", "ml-1"] %>
<% end %>
<%= if is_omni_poa_bridge?(@address_hash) do %>
  <%= render BlockScoutWeb.FormView, "_tag.html", text: "POA", additional_classes: ["destination-poa", "ml-1"] %>
<% end %>
<%= if is_xmoon_token?(@address_hash) || is_xbrick_token?(@address_hash) do %>
  <%= render BlockScoutWeb.FormView, "_tag.html", text: "bridged", additional_classes: ["bridge", "ml-1"] %>
  <%= render BlockScoutWeb.FormView, "_tag.html", text: "Rinkeby", additional_classes: ["destination-rinkeby", "ml-1"] %>
<% end %>
<%= for tag <- tags do %>
  <%= if tag.label =~ "dark forest" do %>
    <%= render BlockScoutWeb.FormView, "_tag.html", text: tag.display_name, additional_classes: ["df", "ml-1"] %>
  <% else %>
    <%= render BlockScoutWeb.FormView, "_tag.html", text: tag.display_name, additional_classes: [tag_name_to_label(tag.label), "ml-1"] %>
  <% end %>
<% end %>
<% dark_forest_addresses_list_dao = CustomContractsHelpers.get_custom_addresses_list(:dark_forest_addresses_dao) %>
<%= if Enum.member?(dark_forest_addresses_list_dao, String.downcase(to_string(@address_hash))) do %>
  <%= render BlockScoutWeb.FormView, "_tag.html", text: "â˜ ï¸ðŸª", additional_classes: ["df", "df-dao", "ml-1"] %>
<% end %>
<% private_tags = if assigns[:private_tags], do: @private_tags, else: [] %>
<%= for private_tag <- private_tags do %>
  <%= if private_tag.label =~ "dark forest" do %>
    <%= render BlockScoutWeb.FormView, "_tag.html", text: private_tag.display_name, additional_classes: ["df", "ml-1"] %>
  <% else %>
    <%= render BlockScoutWeb.FormView, "_tag.html", text: private_tag.display_name, additional_classes: [tag_name_to_label(private_tag.label), "ml-1"] %>
  <% end %>
<% end %>