<div class="tile tile-muted tile-function-response monospace">
  [ <%= @function_name %> method Response ]

  <p class="text-dark">
    [
      <%= for item <- @outputs do %>
        <span class="text-dark function-response-item"><%= if named_argument?(item) do %><%= item["name"] %><% end %>(<%= item["type"] %>) : <%= item["value"] %></span>
      <% end %>
    ]
  </p>
</div>
